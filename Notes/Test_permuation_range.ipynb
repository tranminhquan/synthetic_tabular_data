   "source": [
    "def cal_lenghts(x):\n",
    "    if x == 1:\n",
    "        return 1\n",
    "    return x + cal_lenghts(x-1)\n",
    "\n",
    "\n",
    "for i in range(1,10):\n",
    "    S = cal_lenghts(i)\n",
    "    print(f\"n={i} - p={S} - p/n={S/i}\")"
   ]
  },
